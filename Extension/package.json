{
	"name": "prueba",
	"displayName": "prueba",
	"description": "new test",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.play"
	],
	"main": "./out/extension.js",
	"contributes": {
		"menus": {
			"view/title": [
				{
					"command": "TreeDataProvider.refreshEntry",
					"when": "view == TreeDataProvider",
					"group": "navigation"
				},
				{
					"command": "TreeDataProvider.load",
					"when": "view == TreeDataProvider",
					"group": "navigation"
				},
				{
					"command": "TreeDataProvider.save",
					"when": "view == TreeDataProvider",
					"group": "navigation"
				}
			]
		},
		"viewsContainers": {
			"activitybar": [
				{
					"id": "Heap-Visualizer",
					"title": "Heap Visualizer",
					"icon": "./resources/icons/battery-full.svg"
				}
			]
		},
		"views": {
			"Heap-Visualizer": [
				{
					"id": "TreeDataProvider",
					"name": "VSPtr"
				}
			]
		},
		"commands": [
			{
				"command": "TreeDataProvider.refreshEntry",
				"title": "Refrescar",
				"icon": {
					"light": "resources/light/refresh.svg",
					"dark": "resources/dark/refresh.svg"
				}
			},
			{
				"command": "TreeDataProvider.load",
				"title": "Load Data",
				"icon": {
					"light": "resources/light/boolean.svg",
					"dark": "resources/dark/boolean.svg"
				}
			},
			{
				"command": "TreeDataProvider.save",
				"title": "Save Data",
				"icon": {
					"light": "resources/light/dependency.svg",
					"dark": "resources/dark/dependency.svg"
				}
			},
			{
				"command": "extension.play",
				"title": "Heap Visualizer"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.44.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.26.0",
		"@typescript-eslint/eslint-plugin": "^2.26.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"ffi": "^2.3.0",
		"ffi-napi": "^2.5.0",
		"ncp": "^2.0.0",
		"path": "^0.12.7"
	}
}
